public class YourSchedule {
    public static void main(String[] args) {
        String course1, course2, course3, course4, teacher1, teacher2, teacher3, teacher4;
        
        course1 = "English";
        course2 = "Music";
        course3 = "Physics";
        course4 = "Computer Science";
        teacher1 = "Ms. Nardi";
        teacher2 = "Mr. DiGirolamo";
        teacher3 = "Mr. Leduc";
        teacher4 = "Mr. Gallo";
        
        System.out.println("+--------------------------------------------------+");
        System.out.println("| 1 |                   " + course1 + " |     " + teacher1 + " |");
        System.out.println("| 2 |                     " + course2 + " |" + teacher2 + " |");
        System.out.println("| 3 |                   " + course3 + " |     " + teacher3 + " |");
        System.out.println("| 4 |         " + course4 + " |     " + teacher4 + " |");
        System.out.println("+--------------------------------------------------+");
    }
}
